<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SignIn">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#181D2D</item>
    </style>

    <style name="WelcomeBack">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#AAAAAA</item>
    </style>

    <style name="NewMember">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#AAAAAA</item>
    </style>

    <style name="CardInfoText">
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="InputProfile">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/oswald_regular</item>
        <item name="background">@null</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="InputIconProfile">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:padding">16dp</item>
    </style>
</resources>
